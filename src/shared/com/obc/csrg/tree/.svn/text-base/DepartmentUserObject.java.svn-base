package com.obc.csrg.tree;

import java.io.Serializable;

import javax.swing.tree.DefaultMutableTreeNode;

import com.obc.csrg.model.Department;

public class DepartmentUserObject extends NodeUserObject implements Serializable{
	
	private Department department;

	public DepartmentUserObject(DefaultMutableTreeNode defaultMutableTreeNode) {
        super(defaultMutableTreeNode);
    }
	
	public Department getDepartment() {
		return department;
	}

	public void setDepartment(Department department) {
		this.department = department;
	}

}
