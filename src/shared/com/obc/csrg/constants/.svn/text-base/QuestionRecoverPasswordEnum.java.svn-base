package com.obc.csrg.constants;


public enum QuestionRecoverPasswordEnum {
	cidadeNascimento,nomeConjuge,nomeFilho,corCarro,mesNascimento,nomeSolteiraMae,animalPreferido,corFavorita,equipaFutebol;
	
	public String getName(){
		
		switch(this.ordinal()){
		case 0:return Constants.QUESTION_RECOVER_PASSWORD_CIDADE_NASCIMENTO;
		case 1:return Constants.QUESTION_RECOVER_PASSWORD_NOME_CONJUGE;
		case 2:return Constants.QUESTION_RECOVER_PASSWORD_NOME_FILHO;
		case 3:return Constants.QUESTION_RECOVER_PASSWORD_COR_CARRO;
		case 4:return Constants.QUESTION_RECOVER_PASSWORD_MES_NASCIMENTO;
		case 5:return Constants.QUESTION_RECOVER_PASSWORD_NOME_SOLTEIRA_MAE;
		case 6:return Constants.QUESTION_RECOVER_PASSWORD_ANIMAL_PREFERIDO;
		case 7:return Constants.QUESTION_RECOVER_PASSWORD_COR_FAVORITA;
		case 8:return Constants.QUESTION_RECOVER_PASSWORD_EQUIPA_FUTEBOL;
		default :return Constants.QUESTION_RECOVER_PASSWORD_CIDADE_NASCIMENTO;
		}
	}

}
