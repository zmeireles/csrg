<ui:composition  xmlns="http://www.w3.org/1999/xhtml"
                 xmlns:ui="http://java.sun.com/jsf/facelets"
                 xmlns:h="http://java.sun.com/jsf/html"
                 xmlns:f="http://java.sun.com/jsf/core"
                 xmlns:ice="http://www.icesoft.com/icefaces/component"
                 xmlns:s="http://jboss.com/products/seam/taglib">
                 
    <ice:panelGrid columns="2" cellpadding="10" style="vertical-align: top">
		<ice:panelGroup>
			<s:decorate id="paginator" template="/layout/icePaginator.xhtml">
				<ice:dataTable id="#{avTableId}" value="#{avDataTableValue}" var="data" rows="10">
					<ice:column style="width: 10px">
		    			<f:facet name="header">
		      				<ice:outputText value="" />
		    			</f:facet>
		    			<h:selectBooleanCheckbox value="#{data.selected}"/>
		    		</ice:column>
		    		<ice:column>
		    			<f:facet name="header">
		      				<ice:outputText value="#{columnHeader}" />
		    			</f:facet>
		    			<ui:insert name="columnValue"/>
		    		</ice:column>
				</ice:dataTable>
				<ui:param name="dataTable" value="#{avTableId}"></ui:param>
			</s:decorate>
			<ui:insert name="avItemsButtons"/>
		</ice:panelGroup>
		<ice:panelGroup>
			<s:decorate id="paginator1" template="/layout/icePaginator.xhtml">
				<ice:dataTable id="#{selTableId}" value="#{selDataTableValue}" var="data" rows="10">
					<ice:column style="width: 10px">
		    			<f:facet name="header">
		      				<ice:outputText value="" />
		    			</f:facet>
		    			<h:selectBooleanCheckbox value="#{data.selected}"/>
		    		</ice:column>
		    		<ice:column>
		    			<f:facet name="header">
		      				<ice:outputText value="#{columnHeader}" />
		    			</f:facet>
		    			<ui:insert name="columnValue"/>
		    		</ice:column>
				</ice:dataTable>
				<ui:param name="dataTable" value="#{selTableId}"></ui:param>
			</s:decorate>
			<ui:insert name="selItemsButtons"/>
		</ice:panelGroup>
	</ice:panelGrid>
    <ui:insert/><!-- insert xhtml code not included in the ui:define but inside the s:decorate tag -->
</ui:composition>