package com.obc.csrg.constants;

public enum CivilStateEnum {
	married,single,other;
	
	public String getName(){
		
		switch(this.ordinal()){
		case 0:return Constants.CIVIL_STATE_MARRRIED;
		case 1:return Constants.CIVIL_STATE_SINGLE;
		case 2:return Constants.CIVIL_STATE_OTHER;
		default :return Constants.CIVIL_STATE_SINGLE;
		}
	}
}
